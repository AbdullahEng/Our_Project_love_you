@model AdmissionSystem.View_Model.Department_faculty_view_model

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Department_faculty_view_model</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="specialization_name" class="control-label"></label>
                <input asp-for="specialization_name" class="form-control" />
